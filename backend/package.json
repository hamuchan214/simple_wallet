{
  "name": "backend",
  "version": "0.0.1",
  "packageManager": "yarn@4.5.1",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "typesync || :",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch ./src/index.ts",
    "clean": "rimraf ./dist/",
    "tsc": "tsc",
    "build": "run-s clean tsc",
    "start": "node .",
    "prisma": "pnpify prisma",
    "generate": "pnpify prisma generate",
    "migrate": "pnpify prisma migrate dev",
    "reset": "pnpify prisma migrate reset",
    "seed": "tsx src/seed.ts",
    "create:view": "sqlite3 prisma/wallet.db \".read prisma/createView.sql\""
  },
  "devDependencies": {
    "@types/bcrypt": "~10.0.28-alpha",
    "@types/cors": "~2.8.17",
    "@types/express": "~5.0.0",
    "@types/jsonwebtoken": "~9.0.7",
    "@types/node": "^22.10.2",
    "@yarnpkg/pnpify": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prisma": "6.2.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typesync": "^0.14.0"
  },
  "dependencies": {
    "@prisma/client": "6.2.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1"
  }
}
